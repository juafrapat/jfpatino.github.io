AUTHOR = {Franco-Patino, J. M. and González-Jiménez, R. and Dolan, S. and Barbaro, M. B. and Caballero, J. A. and Megias, G. D. and Udias, J. M.},
TITLE = {Analysis of T2K and MINER&nu;A Semi-Inclusive &nu;&mu;&minus;12C Measurements},
